{
  "files.eol": "\n",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "python.analysis.typeCheckingMode": "basic",
  "git.enableSmartCommit": true
}